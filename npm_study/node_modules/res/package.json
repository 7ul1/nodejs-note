{
  "name": "res",
  "description": "Device resolution detection module",
  "version": "0.4.0",
  "homepage": "https://github.com/ryanve/res",
  "license": "MIT",
  "author": "Ryan Van Etten",
  "keywords": [
    "device",
    "screen",
    "browser",
    "resolution",
    "detection",
    "responsive",
    "retina",
    "device-pixel-ratio",
    "ender",
    "mobile",
    "detect",
    "dpr"
  ],
  "main": "res.js",
  "scripts": {
    "pretest": "jshint res.js && jshint test.js",
    "test": "node test.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ryanve/res.git"
  },
  "devDependencies": {
    "actual": "^0.2.0",
    "aok": "^1.9.0",
    "jshint": "^2.9.3",
    "sos": "0.0.1"
  },
  "jshintConfig": {
    "asi": true,
    "boss": true,
    "browser": true,
    "eqnull": true,
    "expr": true,
    "lastsemic": true,
    "latedef": "nofunc",
    "laxcomma": true,
    "maxcomplexity": 10,
    "maxerr": 4,
    "maxlen": 115,
    "maxparams": 4,
    "node": true,
    "sub": true,
    "supernew": true,
    "trailing": true,
    "undef": true,
    "unused": true
  }
}
